cd /tmp/

wget https://bintray.com/jfrog/artifactory/download_file?file_path=jfrog-artifactory-oss-4.16.0.zip -O /tmp/artifactory.zip
unzip artifactory.zip
rm -fr /tmp/artifactory.zip

mv artifactory-oss-4.16.0/bin artifactory-oss-4.16.0/bin_art
rm -fr artifactory-oss-4.16.0/tomcat/conf/server.xml
mv artifactory-oss-4.16.0/* $OPENSHIFT_ARTIFACTORY_DIR/